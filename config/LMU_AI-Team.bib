	number = {04/05},
	urldate = {2022-08-26},
	journal = {Methods of Information in Medicine},
	author = {Garavand, Ali and Samadbeik, Mahnaz and Nadri, Hamed and Rahimi, Bahlol and Asadi, Heshmatollah},
	month = nov,
	year = {2019},
	pages = {131--139},
	file = {Garavand et al. - 2019 - Effective Factors in Adoption of Mobile Health App.pdf:/Users/anne/Zotero/storage/V92JVSZQ/Garavand et al. - 2019 - Effective Factors in Adoption of Mobile Health App.pdf:application/pdf},
