	file = {Karyotaki et al. - 2015 - Predictors of treatment dropout in self-guided web.pdf:/Users/anne/Zotero/storage/GYSMRGFI/Karyotaki et al. - 2015 - Predictors of treatment dropout in self-guided web.pdf:application/pdf},
}

