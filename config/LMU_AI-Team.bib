	author = {Bzdok, Danilo and Meyer-Lindenberg, Andreas},
	month = mar,
	year = {2018},
	keywords = {Artificial intelligence, Machine learning, Endophenotypes, Null-hypothesis testing, Personalized medicine, Predictive analytics, Research Domain Criteria (RDoC), Single-subject prediction},
