  year = {2020},
  journal = {Nature Medicine},
  volume = {26},
  number = {8},
  pages = {1183--1192},
  publisher = {{Nature Research}},
  address = {{United States}},
  issn = {1078-8956},
  doi = {10.1038/s41591-020-1011-4},
  abstract = {Digital technologies are being harnessed to support the public-health response to COVID-19 worldwide, including population surveillance, case identification, contact tracing and evaluation of interventions on the basis of mobility data and communication with the public. These rapid responses leverage billions of mobile phones, large online datasets, connected devices, relatively low-cost computing resources and advances in machine learning and natural language processing. This Review aims to capture the breadth of digital innovations for the public-health response to COVID-19 worldwide and their limitations, and barriers to their implementation, including legal, ethical and privacy barriers, as well as organizational and workforce barriers. The future of public health is likely to become increasingly digital, and we review the need for the alignment of international strategies for the regulation, evaluation and use of digital technologies to strengthen pandemic management, and future preparedness for COVID-19 and other infectious diseases.Copyright  2020, Springer Nature America, Inc.},
  langid = {english},
  keywords = {*coronavirus disease 2019,*medical technology,*public health service,contact examination,data quality,data visualization,decision support system,disease surveillance,human,interpersonal communication,medical ethics,pandemic,priority journal,privacy,review,workforce}
}

@article{burrish_a_21,
  title = {Incorporating {{AI}} into Oncology Practice: {{Opportunities}} and Challenges},
  author = {{Burris H.A.}},
  year = {2021},
  journal = {Annals of Oncology},
  series = {{{JSMO2021 Virtual Congress}} 2021 the {{Japanese Society}} of {{Medical Oncology Annual Meeting}}. {{Virtual}}, {{Online}}.},
  volume = {32},
  number = {Supplement 4},
  pages = {S232},
  publisher = {{Elsevier Ltd}},
  address = {{Netherlands}},
  issn = {0923-7534},
  doi = {10.1016/j.annonc.2021.05.370},
  abstract = {Artificial intelligence (AI) is rapidly being incorporated and adopted into the cancer field and is having an impact on drug discovery, trial recruitment, early diagnosis, and patient care. The use of bioinformatics platforms to handle the massive amounts of data coming from next generation sequencing and offer decision support to clinicians is critical to appropriately managing patients. The molecular profiling of cancers, particularly in the relapsed and refractory setting, is necessary for the triage decision of the two promising trends in treatment, either identifying an actionable mutation or the biomarkers predicting success with immunotherapy. In addition, this data is proving to be valuable in the area of clinical trial matching for patients and in discovering resistance patterns to various therapies. AI is now playing a greater role in early diagnosis, including its implementation into imaging fields such as mammography. The use of natural language processing is aiding in the detection of cancers from incidental lung nodules from routine imaging and in interpreting pathology reports for greater efficiencies with nurse navigation and cancer registry work. Investments in rapidly changing technology, hiring the necessary expertise, and implementing across diverse systems are just of some the challenges faced by healthcare systems.Copyright  2021 European Society for Medical Oncology},
  langid = {english},
  keywords = {adult,artificial intelligence,bioinformatics,biological marker,cancer recurrence,cancer registry,cancer resistance,conference abstract,controlled study,decision support system,diagnosis,early diagnosis,emergency health service,genetic marker,health care system,high throughput sequencing,human,immunotherapy,investment,lung nodule,mammography,molecular fingerprinting,natural language processing,nurse,patient care}
}

@article{bzdok_meyer-lindenberg18,
  title = {Machine {{Learning}} for {{Precision Psychiatry}}: {{Opportunities}} and {{Challenges}}},
