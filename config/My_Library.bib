  keywords = {*non insulin dependent diabetes mellitus,allergy,article,body mass,coronary artery atherosclerosis,decision support system,dipeptidyl carboxypeptidase inhibitor/ec [Endogenous Compound],human,hypertension,immunization,loop diuretic agent,machine learning,medical record,patient care,prediction,priority journal}
}

