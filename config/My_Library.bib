@article{jenkinsp_r__etal21,
  title = {Optimising Aerial Military Medical Evacuation Dispatching Decisions via Operations Research Techniques},
  author = {{Jenkins P.R.} and {Robbins M.J.} and {Lunday B.J.}},
  year = {2021},
  journal = {BMJ military health},
  number = {(Jenkins, Robbins, Lunday) Operational Sciences, Air Force Institute of Technology, Wright-Patterson AFB, OH, United States},
  publisher = {{NLM (Medline)}},
  address = {{United Kingdom}},
  issn = {2633-3775 (electronic)},
  doi = {10.1136/bmjmilitary-2020-001631},
  abstract = {Senior military leaders and medical practitioners continuously seek new ways to improve the performance and organisation of deployed medical evacuation (MEDEVAC) systems to minimise mortality rates of combat casualties. The objective of this paper is to highlight how recent research in the fields of operations research and machine learning can be leveraged to better inform the implementation and modification of current and future MEDEVAC tactics, techniques and procedures for combat operations in a deployed environment. More specifically, this paper discusses state-of-the-art techniques that optimise the management of MEDEVAC assets prior to and during combat operations. These recent research efforts emphasise that military healthcare administrators should contribute to and extend the evolving portfolio of research that seeks to design and develop decision support systems leveraging artificial intelligence and operations research to improve MEDEVAC system performance.Copyright  Author(s) (or their employer(s)) 2021. No commercial re-use. See rights and permissions. Published by BMJ.},
  langid = {english},
  keywords = {*accident,*army,*emergency medicine,*injury,*system analysis,administrative personnel,article,artificial intelligence,decision support system,human,machine learning}
}

@article{ji_etal21,
  title = {Status of {{AI-Enabled Clinical Decision Support Systems Implementations}} in {{China}}.},
  author = {Ji, Mengting and Chen, Xiaoyun and Genchev, Georgi Z and Wei, Mingyue and Yu, Guangjun},
  year = {2021},
  journal = {Methods of information in medicine},
  volume = {60},
  number = {5-06},
  pages = {123--132},
  address = {{Germany}},
  issn = {2511-705X},
  doi = {10.1055/s-0041-1736461},
  abstract = {BACKGROUND: AI-enabled Clinical Decision Support Systems (AI + CDSSs) were heralded to contribute greatly to the advancement of health care services. There is an increased availability of monetary funds and technical expertise invested in projects and proposals targeting the building and implementation of such systems. Therefore, understanding the actual system implementation status in clinical practice is imperative., OBJECTIVES: The aim of the study is to understand (1) the current situation of AI + CDSSs clinical implementations in Chinese hospitals and (2) concerns regarding AI + CDSSs current and future implementations., METHODS: We investigated 160 tertiary hospitals from six provinces and province-level cities. Descriptive analysis, two-sided Fisher exact test, and Mann-Whitney U-test were utilized for analysis., RESULTS: Thirty-eight of the surveyed hospitals (23.75\%) had implemented AI + CDSSs. There were statistical differences on grade, scales, and medical volume between the two groups of hospitals (implemented vs. not-implemented AI + CDSSs, p {$<$}0.05). On the 5-point Likert scale, 81.58\% (31/38) of respondents rated their overall satisfaction with the systems as "just neutral" to "satisfied." The three most common concerns were system functions improvement and integration into the clinical process, data quality and availability, and methodological bias., CONCLUSION: While AI + CDSSs were not yet widespread in Chinese clinical settings, professionals recognize the potential benefits and challenges regarding in-hospital AI + CDSSs. Copyright Thieme. All rights reserved.},
  keywords = {*clinical decision support system,*Decision Support Systems; Clinical,artificial intelligence,Artificial Intelligence,China,hospital,Hospitals,questionnaire,Surveys and Questionnaires},
  annotation = {Ji, Mengting. Department of Oncology, Ren Ji Hospital, School of Medicine, Shanghai Jiao Tong University, Shanghai, People's Republic of China. Chen, Xiaoyun. China Digital Medicine Press, Beijing, People's Republic of China. Genchev, Georgi Z. Department of Information Technology, Shanghai Children's Hospital, Shanghai, People's Republic of China. Genchev, Georgi Z. Bulgarian Institute for Genomics and Precision Medicine, Sofia, Bulgaria. Wei, Mingyue. Department of Information Technology, Shanghai Children's Hospital, Shanghai, People's Republic of China. Yu, Guangjun. Department of Information Technology, Shanghai Children's Hospital, Shanghai, People's Republic of China.}
}

@article{jia_etal20,
  title = {Predicting the {{Regional Adoption}} of {{Electric Vehicle}} ({{EV}}) {{With Comprehensive Models}}},
  author = {Jia, JM and Shi, BY and Che, F and Zhang, H},
  year = {2020},
  journal = {IEEE ACCESS},
  volume = {8},
  pages = {147275--147285},
  issn = {2169-3536},
  doi = {10.1109/ACCESS.2020.3014851},
  abstract = {Adoption of electric vehicles (EVs) has been regarded as one of the most important strategies to address the issues of energy dependence and greenhouse effect. Empirical reviews demonstrate that wide acceptance of EV is still difficult to achieve. This research proposes to investigate the factors that might trigger the wide usage of EVs to support the energy policy. The real-world owners of EV were extracted from the 2017 National Household Travel Survey (NHTS), which provides large-scale individual characteristics. NHTS dataset was processed to establish the comprehensive estimation model for EV adoption with considering vehicle, personal and household factors. Besides the commonly social-economic factors, the gasoline price and car sharing program were found to be significant for EV adoption. Additionally, since the EV owners are only 1.29\% of all vehicle owners, this article introduced the imbalanced dataset technique, which was seldom considered in existing researches. Subsequently, several machine learning methods were utilized to build the prediction model, and the model performance analysis indicates the Decision Tree (DT) model outperforms other models. A regional EV penetration map was also generated for the U.S. to validate the proposed approach. Implications for further research, transport policy and EV market are discussed.},
  langid = {english},
  keywords = {2017 NHTS,Analytical models,Batteries,Biological system modeling,CHARGING STATIONS,CHOICE ANALYSIS,comprehensive models,Economics,EV adoption,HYBRID,imbalanced dataset,INCENTIVES,INTENTION,LANDSLIDE SUSCEPTIBILITY,LOCATIONS,MARKET,Petroleum,Predictive models,PREFERENCES,PURCHASE,socio-economic factors,Transportation}
}

@article{jian_etal22a,
  title = {Voice-Based Control System for Smart Hospital Wards: A Pilot Study of Patient Acceptance},
  author = {Jian, WS and Wang, JY and Rahmanti, AR and Chien, SC and Hsu, CK and Chien, CH and Li, YC and Chen, CY and Chin, YP and Huang, CL},
  year = {2022},
  month = mar,
  journal = {BMC HEALTH SERVICES RESEARCH},
  volume = {22},
  number = {1},
  issn = {1472-6963},
  doi = {10.1186/s12913-022-07668-1},
  abstract = {Background The smart hospital's concept of using the Internet of Things (IoT) to reduce human resources demand has become more popular in the aging society. Objective To implement the voice smart care (VSC) system in hospital wards and explore patient acceptance via the Technology Acceptance Model (TAM). Methods A structured questionnaire based on TAM was developed and validated as a research tool. Only the patients hospitalized in the VSC wards and who used it for more than two days were invited to fill the questionnaire. Statistical variables were analyzed using SPSS version 24.0. A total of 30 valid questionnaires were finally obtained after excluding two incomplete questionnaires. Cronbach's alpha values for all study constructs were above 0.84. Result We observed that perceived ease of use on perceived usefulness, perceived usefulness on user satisfaction and attitude toward using, and attitude toward using on behavioral intention to use had statistical significance (p {$<$} .01), respectively. Conclusion We have successfully developed the VSC system in a Taiwanese academic medical center. Our study indicated that perceived usefulness was a crucial factor, which means the system function should precisely meet the patients' demands. Additionally, a clever system design is important since perceived ease of use positively affects perceived usefulness. The insight generated from this study could be beneficial to hospitals when implementing similar systems to their wards.},
  langid = {english},
  keywords = {INTENTION,Patient autonomy,SIZE,Smart hospital,Technology acceptance model (TAM),Voice-based control system}
}

