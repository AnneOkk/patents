@article{saez_etal16,
  title = {{{INFFC}}: {{An}} Iterative Class Noise Filter Based on the Fusion of Classifiers with Noise Sensitivity Control},
  author = {Saez, JA and Galar, M and Luengo, J and Herrera, F},
  year = {2016},
