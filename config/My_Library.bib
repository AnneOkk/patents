  author = {De Ramon Fernandez, Alberto and Ruiz Fernandez, Daniel and Prieto Sanchez, Maria Teresa},
  year = {2022},
  journal = {Computer methods and programs in biomedicine},
  volume = {219},
  number = {doh, 8506513},
  pages = {106740},
