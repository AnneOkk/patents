  keywords = {adaptive deep belief networks,Distributed deep learning,extreme learning machine,extreme transfer learning,federated learning,GAME,GO,ladder networks,MACHINE,MODEL,NETWORKS,regenerative chaining,reinforcement learning,representation learning,self-supervision,semi-supervised learning,zero shot learning}
