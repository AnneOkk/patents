@article{zerdoumi_etal18,
  title = {Image Pattern Recognition in Big Data: Taxonomy and Open Challenges: Survey},
  author = {Zerdoumi, S and Sabri, AQM and Kamsin, A and Hashem, IAT and Gani, A and Hakak, S and {Al-Garadi}, MA and Chang, V},
