  title = {Teleophthalmology-Enabled and Artificial Intelligence-Ready Referral Pathway for Community Optometry Referrals of Retinal Disease ({{HERMES}}): A {{Cluster Randomised Superiority Trial}} with a Linked {{Diagnostic Accuracy Study-HERMES}} Study Report 1-Study Protocol.},
  author = {Han, Ji Eun Diana and Liu, Xiaoxuan and Bunce, Catey and Douiri, Abdel and Vale, Luke and Blandford, Ann and Lawrenson, John and Hussain, Rima and Grimaldi, Gabriela and Learoyd, Annastazia E and Kernohan, Ashleigh and Dinah, Christiana and Minos, Evangelos and Sim, Dawn and Aslam, Tariq and Patel, Praveen J and Denniston, Alastair K and Keane, Pearse A and Balaskas, Konstantinos},
