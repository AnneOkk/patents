@article{harveyh__etal19,
  title = {The {{Role}} of {{Deep Learning}} in {{Breast Screening}}},
  author = {{Harvey H.} and {Karpati E.} and {Khara G.} and {Korkinof D.} and {Ng A.} and {Austin C.} and {Rijken T.} and {Kecskemethy P.}},
