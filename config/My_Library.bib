@article{rajar__etal21,
  title = {A {{Machine Learning-Based Prediction Model}} for {{Preterm Birth}} in {{Rural India}}},
  author = {{Raja R.} and {Mukherjee I.} and {Sarkar B.K.}},
  year = {2021},
  journal = {Journal of Healthcare Engineering},
  volume = {2021},
  number = {(Raja, Mukherjee, Sarkar) Department of Computer Science and Engineering, Birla Institute of Technology, Mesra, Ranchi, India},
  pages = {6665573},
  publisher = {{Hindawi Limited}},
  address = {{United States}},
  issn = {2040-2295},
  doi = {10.1155/2021/6665573},
  abstract = {Preterm birth (PTB) in a pregnant woman is the most serious issue in the field of Gynaecology and Obstetrics, especially in rural India. In recent years, various clinical prediction models for PTB have been developed to improve the accuracy of learning models. However, to the best of the authors' knowledge, most of them suffer from selecting the most accurate features from the medical dataset in linear time. The present paper attempts to design a machine learning model named as risk prediction conceptual model (RPCM) for the prediction of PTB. In this paper, a feature selection approach is proposed based on the notion of entropy. The novel approach is used to find the best maternal features (responsible for PTB) from the obstetrical dataset and aims to predict the classifier's accuracy at the highest level. The paper first deals with the review of PTB cases (which is neglected in many developing countries including India). Next, we collect obstetrical data from the Community Health Centre of rural areas (Kamdara, Jharkhand). The suggested approach is then applied on collected data to identify the excellent maternal features (text-based symptoms) present in pregnant women in order to classify all birth cases into term birth and PTB. The machine learning part of the model is implemented using three different classifiers, namely, decision tree (DT), logistic regression (LR), and support vector machine (SVM) for PTB prediction. The performance of the classifiers is measured in terms of accuracy, specificity, and sensitivity. Finally, the SVM classifier generates an accuracy of 90.9\%, which is higher than other learning classifiers used in this study.Copyright  2021 Rakesh Raja et al.},
  langid = {english},
  keywords = {*computer model,*conceptual model,*machine learning,*prediction,*premature labor/di [Diagnosis],*support vector machine,article,clinical classification,controlled study,decision support system,decision tree,diagnostic accuracy,diagnostic test accuracy study,entropy,feature selection,female,human,incidence,information retrieval,information service,logistic regression analysis,major clinical study,measurement accuracy,predictive value,sensitivity and specificity,symptom assessment}
}

@article{rajkomar_etal16,
  title = {Weighting {{Primary Care Patient Panel Size}}: {{A Novel Electronic Health Record-Derived Measure Using Machine Learning}}},
  author = {Rajkomar, A and Yim, JWL and Grumbach, K and Parekh, A},
  year = {2016},
  month = oct,
  journal = {JMIR MEDICAL INFORMATICS},
  volume = {4},
  number = {4},
  pages = {3--15},
  issn = {2291-9694},
  doi = {10.2196/medinform.6530},
  abstract = {Background: Characterizing patient complexity using granular electronic health record (EHR) data regularly available to health systems is necessary to optimize primary care processes at scale. Objective: To characterize the utilization patterns of primary care patients and create weighted panel sizes for providers based on work required to care for patients with different patterns. Methods: We used EHR data over a 2-year period from patients empaneled to primary care clinicians in a single academic health system, including their in-person encounter history and virtual encounters such as telephonic visits, electronic messaging, and care coordination with specialists. Using a combination of decision rules and k-means clustering, we identified clusters of patients with similar health care system activity. Phenotypes with basic demographic information were used to predict future health care utilization using log-linear models. Phenotypes were also used to calculate weighted panel sizes. Results: We identified 7 primary care utilization phenotypes, which were characterized by various combinations of primary care and specialty usage and were deemed clinically distinct by primary care physicians. These phenotypes, combined with age-sex and primary payer variables, predicted future primary care utilization with R-2 of .394 and were used to create weighted panel sizes. Conclusions: Individual patients' health care utilization may be useful for classifying patients by primary care work effort and for predicting future primary care usage.},
  langid = {english},
  keywords = {ambulatory care,health care economics and organizations,machine learning,medical informatics,patient acceptance of health care,primary health care,risk adjustment}
}

@article{rajkomar_etal18,
  title = {Ensuring {{Fairness}} in {{Machine Learning}} to {{Advance Health Equity}}.},
  author = {Rajkomar, Alvin and Hardt, Michaela and Howell, Michael D and Corrado, Greg and Chin, Marshall H},
  year = {2018},
  journal = {Annals of internal medicine},
  series = {Comment in: {{Ann Intern Med}}. 2018 {{Dec}} 18;169(12):883-884 {{PMID}}: 30508423 [{{https://www.ncbi.nlm.nih.gov/pubmed/30508423]}}},
  volume = {169},
  number = {12},
  pages = {866--872},
  address = {{United States}},
  issn = {1539-3704},
  doi = {10.7326/M18-1990},
  abstract = {Machine learning is used increasingly in clinical care to improve diagnosis, treatment selection, and health system efficiency. Because machine-learning models learn from historically collected data, populations that have experienced human and structural biases in the past-called protected groups-are vulnerable to harm by incorrect predictions or withholding of resources. This article describes how model design, biases in data, and the interactions of model predictions with clinicians and patients may exacerbate health care disparities. Rather than simply guarding against these harms passively, machine-learning systems should be used proactively to advance health equity. For that goal to be achieved, principles of distributive justice must be incorporated into model design, deployment, and evaluation. The article describes several technical implementations of distributive justice-specifically those that ensure equality in patient outcomes, performance, and resource allocation-and guides clinicians as to when they should prioritize each principle. Machine learning is providing increasingly sophisticated decision support and population-level monitoring, and it should encode principles of justice to ensure that models benefit all patients.},
  keywords = {*health equity,*Health Equity,*Healthcare Disparities,*machine learning,*Machine Learning,adult,article,clinician,Critical Care,decision support system,diagnosis,health care disparity,Health Care Rationing,human,Humans,justice,Length of Stay,Machine Learning/st [Standards],monitoring,Patient Outcome Assessment,prediction,resource allocation,Social Justice},
  annotation = {Rajkomar, Alvin. Google, Mountain View, California, and University of California, San Francisco, San Francisco, California (A.R.). Hardt, Michaela. Google, Mountain View, California (M.H., M.D.H., G.C.). Howell, Michael D. Google, Mountain View, California (M.H., M.D.H., G.C.). Corrado, Greg. Google, Mountain View, California (M.H., M.D.H., G.C.). Chin, Marshall H. University of Chicago, Chicago, Illinois (M.H.C.).}
}

