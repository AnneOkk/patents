@article{papageorgiou_etal06,
  title = {A Combined {{Fuzzy Cognitive Map}} and Decision Trees Model for Medical Decision Making.},
  author = {Papageorgiou, Elpiniki and Stylios, Chrysostomos and Groumpos, Peter},
