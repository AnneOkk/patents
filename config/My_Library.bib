  author = {Petersen, Carolyn and Smith, Jeffery and Freimuth, Robert R and Goodman, Kenneth W and Jackson, Gretchen Purcell and Kannry, Joseph and Liu, Hongfang and Madhavan, Subha and Sittig, Dean F and Wright, Adam},
