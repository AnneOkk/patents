@article{biesialska_etal21,
  title = {Big {{Data}} Analytics in {{Agile}} Software Development: {{A}} Systematic Mapping Study},
  author = {Biesialska, K and Franch, X and {Muntes-Mulero}, V},
  year = {2021},
  month = apr,
  journal = {INFORMATION AND SOFTWARE TECHNOLOGY},
  volume = {132},
  issn = {0950-5849},
  doi = {10.1016/j.infsof.2020.106448},
  abstract = {Context: Over the last decade, Agile methods have changed the software development process in an unparalleled way and with the increasing popularity of Big Data, optimizing development cycles through data analytics is becoming a commodity. Objective: Although a myriad of research exists on software analytics as well as on Agile software development (ASD) practice on itself, there exists no systematic overview of the research done on ASD from a data analytics perspective. Therefore, the objective of this work is to make progress by linking ASD with Big Data analytics (BDA). Method: As the primary method to find relevant literature on the topic, we performed manual search and snowballing on papers published between 2011 and 2019. Results: In total, 88 primary studies were selected and analyzed. Our results show that BDA is employed throughout the whole ASD lifecycle. The results reveal that data-driven software development is focused on the following areas: code repository analytics, defects/bug fixing, testing, project management analytics, and application usage analytics. Conclusions: As BDA and ASD are fast-developing areas, improving the productivity of software development teams is one of the most important objectives BDA is facing in the industry. This study provides scholars with information about the state of software analytics research and the current trends as well as applications in the business environment. Whereas, thanks to this literature review, practitioners should be able to understand better how to obtain actionable insights from their software artifacts and on which aspects of data analytics to focus when investing in such initiatives.},
  langid = {english},
  keywords = {Agile software development,Artificial intelligence,Data analytics,DATA SCIENCE,DELAY,DRIVEN,LESSONS,Literature review,Machine learning,PRIORITIZATION,Software analytics}
}

@article{bigorral__etal18,
  title = {Machine Learning Algorithms for Accurate Differential Diagnosis of Lymphocytosis Based on Cell Population Data},
  author = {{Bigorra L.} and {Larriba I.} and {Gutierrez-Gallego R.}},
  year = {2018},
  journal = {International Journal of Laboratory Hematology},
  series = {31st {{Annual Meeting}} of the {{International Society}} for {{Laboratory Hematology}}. {{Brussels Belgium}}.},
  volume = {40},
  number = {Supplement 2},
  pages = {18},
  publisher = {{Blackwell Publishing Ltd}},
  address = {{Netherlands}},
  issn = {1751-553X},
  abstract = {Introduction: Cell population data (CPD) parameters from Beckman-Coulter DXH800 analyzers provide volume, conductivity and scatter measures for leucocyte subpopulations. In the present study, fourteen lymphoid CPD parameters were used to design, develop and implement novel machine learning (ML) algorithms as a support tool for lymphocyte related diagnosis. Method(s): Three categories were selected that included healthy controls, reactive lymphocytosis, and chronic lymphocytic leukemia (CLL) samples. A model-building data set (n=300) was used for the feature analysis including 1) ANOVA, 2) unsupervised K-Means clustering, and 3) principal component analysis (PCA) of both unlabeled and labeled data. Secondly, supervised ML algorithms were built for true classification purposes. The different prediction models (6 in total) were built and tested using the model-building dataset, and then evaluated through ten-fold cross validation. The validation was carried out using an independent validation dataset (n=100) covering all three categories. Result(s): 1) The ANOVA revealed that CPD parameters displayed significant differences among the three categories, 2) K-Means clustering segregated the categories in control and pathological groups (for K=2 clusters) or three diagnostic categories (for K=3) revealing association between cluster and specific category. PCA allowed to asses cluster validity and yielded 2 variables (SD conductivity and Mean volume) with significant discriminatory potential. The best model after ten-fold cross validation corresponded to neural network (NN) classifier, with an accuracy of 96.70\%, followed by support vector machines (94.90\%), k-nearest neighbor (94.00\%) and random forest (94.00\%) classifiers. Individual classification accuracies with NN algorithm corresponded to 100\%, 94.5\% and 96.0\% for the controls, reactive, and CLL groups, respectively. Conclusion(s): For five of the tested models the obtained accuracies for the validation dataset were over the 90\%, indicating that the ML algorithms have an impressive discriminatory potential. The NN algorithm appears to be very promising as a support system for medical diagnosis in lymphoproliferative disorders. Longitudinal validation studies with larger patient cohorts will be interesting for the consolidation of these models as a decision support tool in the clinical practice. Notwithstanding the tremendous aid that this algorithm poses for medical diagnosis one should never ease into the pitfall of ruling out human involvement.},
  langid = {english},
  keywords = {*cell population,*differential diagnosis,*lymphocytosis,*support vector machine,adult,analysis of variance,chronic lymphatic leukemia,clinical practice,cohort analysis,conductance,conference abstract,controlled study,decision support system,diagnosis,diagnostic test accuracy study,female,human,human cell,human tissue,k nearest neighbor,major clinical study,male,prediction,principal component analysis,random forest,supervised machine learning,validation process,validation study,validity}
}

@article{biller-andorno_etal22,
  title = {{{AI}} Support for Ethical Decision-Making around Resuscitation: Proceed with Care.},
  author = {{Biller-Andorno}, Nikola and Ferrario, Andrea and Joebges, Susanne and Krones, Tanja and Massini, Federico and Barth, Phyllis and Arampatzis, Georgios and Krauthammer, Michael},
  year = {2022},
  journal = {Journal of medical ethics},
  volume = {48},
  number = {3},
  pages = {175--183},
  address = {{England}},
  issn = {1473-4257},
  doi = {10.1136/medethics-2020-106786},
  abstract = {Artificial intelligence (AI) systems are increasingly being used in healthcare, thanks to the high level of performance that these systems have proven to deliver. So far, clinical applications have focused on diagnosis and on prediction of outcomes. It is less clear in what way AI can or should support complex clinical decisions that crucially depend on patient preferences. In this paper, we focus on the ethical questions arising from the design, development and deployment of AI systems to support decision-making around cardiopulmonary resuscitation and the determination of a patient's Do Not Attempt to Resuscitate status (also known as code status). The COVID-19 pandemic has made us keenly aware of the difficulties physicians encounter when they have to act quickly in stressful situations without knowing what their patient would have wanted. We discuss the results of an interview study conducted with healthcare professionals in a university hospital aimed at understanding the status quo of resuscitation decision processes while exploring a potential role for AI systems in decision-making around code status. Our data suggest that (1) current practices are fraught with challenges such as insufficient knowledge regarding patient preferences, time pressure and personal bias guiding care considerations and (2) there is considerable openness among clinicians to consider the use of AI-based decision support. We suggest a model for how AI can contribute to improve decision-making around resuscitation and propose a set of ethically relevant preconditions-conceptual, methodological and procedural-that need to be considered in further development and implementation efforts. Copyright  Author(s) (or their employer(s)) 2022. No commercial re-use. See rights and permissions. Published by BMJ.},
  keywords = {*artificial intelligence,*Artificial Intelligence,*COVID-19,human,Humans,pandemic,Pandemics,resuscitation,Resuscitation Orders,SARS-CoV-2},
  annotation = {Biller-Andorno, Nikola. Institute of Biomedical Ethics and History of Medicine, Universitat Zurich, Zurich, Switzerland biller-andorno@ibme.uzh.ch. Biller-Andorno, Nikola. Collegium Helveticum, Zurich, Switzerland. Ferrario, Andrea. Department of Management, Technology, and Economics, Eidgenossische Technische Hochschule Zurich, Zurich, Switzerland. Joebges, Susanne. Institute of Biomedical Ethics and History of Medicine, Universitat Zurich, Zurich, Switzerland. Krones, Tanja. Institute of Biomedical Ethics and History of Medicine, Universitat Zurich, Zurich, Switzerland. Krones, Tanja. Clinical Ethics, Universitatsspital Zurich, Zurich, Switzerland. Massini, Federico. Institute of Biomedical Ethics and History of Medicine, Universitat Zurich, Zurich, Switzerland. Massini, Federico. Collegium Helveticum, Zurich, Switzerland. Barth, Phyllis. Institute of Biomedical Ethics and History of Medicine, Universitat Zurich, Zurich, Switzerland. Barth, Phyllis. Collegium Helveticum, Zurich, Switzerland. Arampatzis, Georgios. Collegium Helveticum, Zurich, Switzerland. Arampatzis, Georgios. Computational Science and Engineering Laboratory, Eidgenossische Technische Hochschule Zurich, Zurich, Switzerland. Krauthammer, Michael. Department of Quantitative Biomedicine, Chair of Medical Informatics, Universitat Zurich, Zurich, Switzerland.}
}

@article{bindoff_etal12,
  title = {The Potential for Intelligent Decision Support Systems to Improve the Quality and Consistency of Medication Reviews.},
  author = {Bindoff, I and Stafford, A and Peterson, G and Kang, B H and Tenni, P},
  year = {2012},
  journal = {Journal of clinical pharmacy and therapeutics},
  volume = {37},
  number = {4},
  pages = {452--8},
  address = {{England}},
  issn = {1365-2710},
  doi = {10.1111/j.1365-2710.2011.01327.x},
  abstract = {WHAT IS KNOWN AND OBJECTIVE: Drug-related problems (DRPs) are of serious concern worldwide, particularly for the elderly who often take many medications simultaneously. Medication reviews have been demonstrated to improve medication usage, leading to reductions in DRPs and potential savings in healthcare costs. However, medication reviews are not always of a consistently high standard, and there is often room for improvement in the quality of their findings. Our aim was to produce computerized intelligent decision support software that can improve the consistency and quality of medication review reports, by helping to ensure that DRPs relevant to a patient are overlooked less frequently. A system that largely achieved this goal was previously published, but refinements have been made. This paper examines the results of both the earlier and newer systems., METHODS: Two prototype multiple-classification ripple-down rules medication review systems were built, the second being a refinement of the first. Each of the systems was trained incrementally using a human medication review expert. The resultant knowledge bases were analysed and compared, showing factors such as accuracy, time taken to train, and potential errors avoided., RESULTS AND DISCUSSION: The two systems performed well, achieving accuracies of approximately 80\% and 90\%, after being trained on only a small number of cases (126 and 244 cases, respectively). Through analysis of the available data, it was estimated that without the system intervening, the expert training the first prototype would have missed approximately 36\% of potentially relevant DRPs, and the second 43\%. However, the system appeared to prevent the majority of these potential expert errors by correctly identifying the DRPs for them, leaving only an estimated 8\% error rate for the first expert and 4\% for the second., WHAT IS NEW AND CONCLUSION: These intelligent decision support systems have shown a clear potential to substantially improve the quality and consistency of medication reviews, which should in turn translate into improved medication usage if they were implemented into routine use. Copyright  2011 Blackwell Publishing Ltd.},
  keywords = {*Artificial Intelligence,*decision support system,*Decision Support Techniques,*Drug Utilization Review/mt [Methods],adverse outcome,Aged,drug related problem,drug use,Drug Utilization Review/st [Standards],Drug-Related Side Effects and Adverse Reactions,Health Care Costs,human,Humans,medical expert,medication error,quality control,review,Software},
  annotation = {Bindoff, I. School of Pharmacy, University of Tasmania, Tas., Australia. ivan.bindoff@utas.edu.au}
}

