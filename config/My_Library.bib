@article{wexler_etal20,
  title = {The {{What-If Tool}}: {{Interactive Probing}} of {{Machine Learning Models}}},
  author = {Wexler, J and Pushkarna, M and Bolukbasi, T and Wattenberg, M and Viegas, F and Wilson, J},
