@article{stretch_etal19,
  title = {Predicting {{Nondiagnostic Home Sleep Apnea Tests Using Machine Learning}}.},
  author = {Stretch, Robert and Ryden, Armand and Fung, Constance H and Martires, Joanne and Liu, Stephen and Balasubramanian, Vidhya and Saedi, Babak and Hwang, Dennis and Martin, Jennifer L and Della Penna, Nicolas and Zeidler, Michelle R},
  year = {2019},
  journal = {Journal of clinical sleep medicine : JCSM : official publication of the American Academy of Sleep Medicine},
  volume = {15},
  number = {11},
  pages = {1599--1608},
  address = {{United States}},
  issn = {1550-9397},
  doi = {10.5664/jcsm.8020},
  abstract = {STUDY OBJECTIVES: Home sleep apnea testing (HSAT) is an efficient and cost-effective method of diagnosing obstructive sleep apnea (OSA). However, nondiagnostic HSAT necessitates additional tests that erode these benefits, delaying diagnoses and increasing costs. Our objective was to optimize this diagnostic pathway by using predictive modeling to identify patients who should be referred directly to polysomnography (PSG) due to their high probability of nondiagnostic HSAT., METHODS: HSAT performed as the initial test for suspected OSA within the Veterans Administration Greater Los Angeles Healthcare System was analyzed retrospectively. Data were extracted from pre-HSAT questionnaires and the medical record. Tests were diagnostic if there was a respiratory event index (REI) {$>$}= 5 events/h. Tests with REI {$<$} 5 events/h or technical inadequacy-two outcomes requiring additional testing with PSG-were considered nondiagnostic. Standard logistic regression models were compared with models trained using machine learning techniques., RESULTS: Models were trained using 80\% of available data and validated on the remaining 20\%. Performance was evaluated using partial area under the precision-recall curve (pAUPRC). Machine learning techniques consistently yielded higher pAUPRC than standard logistic regression, which had pAUPRC of 0.574. The random forest model outperformed all other models (pAUPRC 0.862). Preferred calibration of this model yielded the following: sensitivity 0.46, specificity 0.95, positive predictive value 0.81, negative predictive value 0.80., CONCLUSIONS: Compared with standard logistic regression models, machine learning models improve prediction of patients requiring in-laboratory PSG. These models could be implemented into a clinical decision support tool to help clinicians select the optimal test to diagnose OSA. Copyright  2019 American Academy of Sleep Medicine.},
  keywords = {*computer assisted diagnosis,*home care,*machine learning,*Machine Learning,*Polysomnography/is [Instrumentation],*Self Care/is [Instrumentation],*Sleep Apnea; Obstructive/di [Diagnosis],*sleep disordered breathing/di [Diagnosis],adult,Adult,aged,Aged,article,artificial neural network,atrial fibrillation,body mass,Calibration,central sleep apnea syndrome/di [Diagnosis],cerebrovascular accident,chronic obstructive lung disease,comorbidity,congestive heart failure,coronary artery disease,Decision Support Techniques,decision tree,diabetes mellitus,diagnostic test accuracy study,Epworth sleepiness scale,female,Female,human,Humans,hypertension,Insomnia Severity Index,insomnia/di [Diagnosis],k nearest neighbor,major clinical study,male,Male,medical record review,middle aged,Middle Aged,neck circumference,Patient Health Questionnaire 9,polysomnography,Polysomnography/mt [Methods],posttraumatic stress disorder,predictive value,random forest,receiver operating characteristic,respiratory event index,respiratory tract parameters,Retrospective Studies,retrospective study,Self Care/mt [Methods],sensitivity and specificity,Sensitivity and Specificity,support vector machine,Surveys and Questionnaires},
  annotation = {Stretch, Robert. David Geffen School of Medicine at University of California, Los Angeles, California. Stretch, Robert. VA Greater Los Angeles Healthcare System, Los Angeles, California. Ryden, Armand. David Geffen School of Medicine at University of California, Los Angeles, California. Ryden, Armand. VA Greater Los Angeles Healthcare System, Los Angeles, California. Fung, Constance H. David Geffen School of Medicine at University of California, Los Angeles, California. Fung, Constance H. VA Greater Los Angeles Healthcare System, Los Angeles, California. Martires, Joanne. VA Greater Los Angeles Healthcare System, Los Angeles, California. Liu, Stephen. VA Greater Los Angeles Healthcare System, Los Angeles, California. Balasubramanian, Vidhya. VA Greater Los Angeles Healthcare System, Los Angeles, California. Saedi, Babak. VA Greater Los Angeles Healthcare System, Los Angeles, California. Hwang, Dennis. Southern California Permanente Medical Group, Los Angeles, California. Martin, Jennifer L. David Geffen School of Medicine at University of California, Los Angeles, California. Martin, Jennifer L. VA Greater Los Angeles Healthcare System, Los Angeles, California. Della Penna, Nicolas. Laboratory of Computational Physiology at Massachusetts Institute of Technology, Boston, Massachusetts. Zeidler, Michelle R. David Geffen School of Medicine at University of California, Los Angeles, California. Zeidler, Michelle R. VA Greater Los Angeles Healthcare System, Los Angeles, California.}
}

@article{stretchr_j__etal19,
  title = {Use of Machine Learning to Predict Non-Diagnostic Home Sleep Apnea Tests},
  author = {{Stretch R.J.} and {Fung C.H.} and {Ryden A.M.} and {Zeidler M.R.}},
  year = {2019},
  journal = {American Journal of Respiratory and Critical Care Medicine},
  series = {2019 {{International Conference}} of the {{American Thoracic Society}} , {{ATS}} 2019. {{Dallas}}, {{TX United States}}.},
  volume = {199},
  number = {9},
  publisher = {{American Thoracic Society}},
  address = {{Netherlands}},
  issn = {1535-4970},
  abstract = {Rationale: Home sleep apnea testing (HSAT) for obstructive sleep apnea (OSA) is less expensive and more convenient for patients compared to in-lab polysomnography (PSG). Nondiagnostic studies (i.e. AHI {$<$} 5/hr or technically inadequate) erode both these benefits due to need for repeat HSAT or PSG to establish a diagnosis. We explored several predictive modeling approaches to develop an algorithm to predict the pre-test probability of patients having a nondiagnostic HSAT. Method(s): We analyzed data for 614 patients who underwent HSAT using a Respironics StarDust II device as the initial test for OSA at the VA Greater Los Angeles Healthcare System. Only patients without a prior sleep disorder diagnosis or prior sleep study were included. All patients completed a pre-study questionnaire containing multiple validated instruments (i.e. STOPBANG, Epworth Sleepiness Scale, Insomnia Severity Index, Berlin Questionnaire and Brief Restless Legs Questionnaire). Demographics, anthropomorphic measurements and comorbidities were extracted from the VA Computerized Patient Record System. The dataset was split into training and testing sets in a 4:1 ratio via random sampling within each outcome class to maintain the overall class distribution. Missing data was handled via mixed methods including a 'minimum viable data' approach and imputation using k-nearest neighbors (KNN). We compared standard logistic regression - with and without LASSO/Ridge regularization - against machine learning techniques including KNN, gradient boosted decision trees (GBDT) and artificial neural networks (ANN). Model training optimized Area Under the Precision-Recall Curve (AUPRC), a suitable metric for an unbalanced dataset. Validation was performed using k-fold repeated cross-validation on the training dataset, followed by outer validation using the testing dataset. Result(s): As shown in Figure 1, the GBDT model performed best (AUPRC on test dataset: 0.666). Using a probability threshold of 0.55, model sensitivity and specificity were 0.41 and 0.98. Implementing this model to guide testing of every 1,000 patients would result in a 9.4\% (N=123) reduction in total sleep studies performed, characterized by a 13.9\% (N=139) decrease in HSATs and 5.3\% (N=16) increase in PSGs. Furthermore, 40.6\% (N=123) fewer patients would require both HSAT and PSG due to increased yield of first-time sleep studies from 69.7\% to 82.0\%. Conclusion(s): Machine learning can improve efficiency in the diagnosis of OSA. Future work will focus on validation in a larger and more diverse population, followed by implementation into a clinical decision support tool. (Figure Preseted).},
  langid = {english},
  keywords = {*artificial neural network,*sleep disordered breathing,adult,algorithm,California,comorbidity,conference abstract,controlled study,decision support system,decision tree,electronic patient record,endogenous compound,Epworth sleepiness scale,female,Germany,health care system,human,Insomnia Severity Index,k nearest neighbor,major clinical study,male,polysomnography,probability,questionnaire,recall,restless legs syndrome,sensitivity and specificity,sleep disorder,validation process}
}

@article{strohm_etal20,
  title = {Implementation of Artificial Intelligence ({{AI}}) Applications in Radiology: Hindering and Facilitating Factors},
  author = {Strohm, L and Hehakaya, C and Ranschaert, ER and Boon, WPC and Moors, EHM},
  year = {2020},
  month = oct,
  journal = {EUROPEAN RADIOLOGY},
  volume = {30},
  number = {10},
  pages = {5525--5532},
  issn = {0938-7994},
  doi = {10.1007/s00330-020-06946-y},
  abstract = {Objective The objective was to identify barriers and facilitators to the implementation of artificial intelligence (AI) applications in clinical radiology in The Netherlands. Materials and methods Using an embedded multiple case study, an exploratory, qualitative research design was followed. Data collection consisted of 24 semi-structured interviews from seven Dutch hospitals. The analysis of barriers and facilitators was guided by the recently published Non-adoption, Abandonment, Scale-up, Spread, and Sustainability (NASSS) framework for new medical technologies in healthcare organizations. Results Among the most important facilitating factors for implementation were the following: (i) pressure for cost containment in the Dutch healthcare system, (ii) high expectations of AI's potential added value, (iii) presence of hospital-wide innovation strategies, and (iv) presence of a "local champion." Among the most prominent hindering factors were the following: (i) inconsistent technical performance of AI applications, (ii) unstructured implementation processes, (iii) uncertain added value for clinical practice of AI applications, and (iv) large variance in acceptance and trust of direct (the radiologists) and indirect (the referring clinicians) adopters. Conclusion In order for AI applications to contribute to the improvement of the quality and efficiency of clinical radiology, implementation processes need to be carried out in a structured manner, thereby providing evidence on the clinical added value of AI applications.},
  langid = {english},
  keywords = {Artificial intelligence,Computer systems,COMPUTER-AIDED DIAGNOSIS,Computer-assisted,Diagnosis,FUTURE,Information systems,Radiology}
}

@article{sugadev_etal22,
  title = {Implementation of {{Combined Machine Learning}} with the {{Big Data Model}} in {{IoMT Systems}} for the {{Prediction}} of {{Network Resource Consumption}} and {{Improving}} the {{Data Delivery}}},
  author = {Sugadev, M and Rayen, SJ and Harirajkumar, J and Rathi, R and Anitha, G and Ramesh, S and Ramaswamy, K},
  year = {2022},
  month = jul,
  journal = {COMPUTATIONAL INTELLIGENCE AND NEUROSCIENCE},
  volume = {2022},
  issn = {1687-5265},
  doi = {10.1155/2022/6510934},
  abstract = {In recent years, health applications based on the internet of medical things have exploded in popularity in smart cities (IoMT). Many real-time systems help both patients and professionals by allowing remote data access and appropriate responses. The major research problems include making timely medical judgments and efficiently managing massive data utilising IoT-based resources. Furthermore, in many proposed solutions, the dispersed nature of data processing openly raises the risk of information leakage and compromises network integrity. Medical sensors are burdened by such solutions, which reduce the stability of real-time transmission systems. As a result, this study provides a machine-learning approach with SDN-enabled security to forecast network resource usage and enhance sensor data delivery. With a low administration cost, the software define network (SDN) design allows the network to resist dangers among installed sensors. It provides an unsupervised machine learning approach that reduces IoT network communication overheads and uses dynamic measurements to anticipate link status and refines its tactics utilising SDN architecture. Finally, the SDN controller employs a security mechanism to efficiently regulate the consumption of IoT nodes while also protecting them against unidentified events. When the number of nodes and data production rate varies, the suggested approach enhances network speed. As a result, to organise the nodes in a cluster, the suggested model uses an iterative centroid technique. By balancing network demand via durable connections, the multihop transmission technique for routing IoT data improves speed while simultaneously lowering the energy hole problem.},
  langid = {english},
  keywords = {ANALYTICS,INTERNET}
}

@article{sukhikhg_t__etal21,
  title = {{The state of and prospects for the introduction of artificial intelligence technologies in obstetric and gynecological practice}},
  author = {{Sukhikh G.T.} and {Davydov D.G.} and {Loginov V.V.} and {Baev O.R.} and {Prikhodko A.M.} and {Sheshko E.L.} and {Chmykhova E.V.}},
  year = {2021},
  journal = {Akusherstvo i Ginekologiya (Russian Federation)},
  volume = {2021},
  number = {2},
  pages = {5--12},
  publisher = {{Bionika Media Ltd.}},
  address = {{Russian Federation}},
  issn = {0300-9092},
  doi = {10.18565/aig.2021.2.5-12},
  abstract = {The authors have carried out a systematic review of the literature devoted to the current state of and prospects for the use of artificial intelligence (AI) in the field of maternal and fetal health. They have revealed the concept of AI and the ways of its development in medicine. It has been noted that AI does not replace a physician, but it is a tool for improving medical activity. The article shows the possibilities of AI use in obstetrics and gynecology and highlights its areas: medical image recognition; prediction and assistance to physicians in determining the diagnosis; creation of recommendation systems for selecting a treatment; robotization of medical manipulations and augmented reality; optimization of the routine functions of healthcare workers; services for interaction and training of physicians and patients. In addition, AI can be used for scientific purposes to understand complex multifactorial mechanisms for the development of diseases; to create disease information models, new classifications of diseases, and models of therapeutic effects. AI is also able to automatically retrieve new medical information from clinical case reports and scientific publications. The paper gives specific examples of developments in these areas. It considers the expected difficulties in introducing AI systems and describes immediate steps for their implementation. The development of AI systems requires physicians' direct participation, including the selection and preparation of data, the formulation of medical tasks, and their translation into the language of machine learning specialists. It is concluded that AI-based applications in obstetrics and gynecology have already become a reality, and in the near future they will reduce the burden on medical professionals, improve the effectiveness of diagnosis, prediction, and treatment, and prevent medical errors. It is promising to use AI in telemedicine systems to provide assistance to physicians and patients outside the locations of large medical complexes. Conclusion. The results of the review can be used to identify promising researches, to develop a national program for AI introduction in obstetric and gynecological practice and in educational programs, and to improve the qualification of healthcare workers.Copyright  A group of authors, 2021.},
  langid = {russian},
  keywords = {*artificial intelligence,*clinical practice,*gynecology,*obstetrics,*telemedicine,article,augmented reality,diagnosis,diagnostic imaging,health care personnel,human,information model,maternal welfare,medical error,medical information,publication,social participation,treatment indication}
}

@article{sullivanOrganizationalStressJob1992,
