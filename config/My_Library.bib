  keywords = {*breast cancer,*cancer screening,*deep learning,artificial intelligence,attitude to health,clinical decision support system,clinical protocol,collaborative care team,computer assisted diagnosis,diagnostic accuracy,digital breast tomosynthesis,digital mammography,health care cost,health program,health workforce,human,image enhancement,review}
