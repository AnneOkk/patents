@article{vanstiphoutr_g_p_m_12,
  title = {Decision Support Systems: {{Requirements}}, Implementation and Evaluation},
  author = {{Van Stiphout R.G.P.M.}},
  year = {2012},
  journal = {Radiotherapy and Oncology},
  series = {2012 {{Annual Conference}} of the {{European Society}} for {{Radiotherapy}} and {{Oncology}}, {{ESTRO}} 31. {{Barcelona Spain}}.},
  volume = {103},
  number = {SUPPL. 1},
  pages = {S66},
  publisher = {{Elsevier Ireland Ltd}},
  issn = {0167-8140},
  doi = {10.1016/S0167-8140%2812%2970509-1},
  abstract = {Treatment decisions are becoming more and more complex for physicians. The amount of data and parameters of the patient and tumor that possibly affect the outcome is still increasing. Clinical trials are still the standard reference for innovation in treatment, but there is not always consensus in the field. Using the computational power of predictions models for treatment outcome may be very useful to assist in clinical decision making and to deal with this increasing data complexity. These models are specialized in finding patterns in the data, in such a way that only the relevant input data will be associated with the output. A large variety of data sources is required to optimally predict the outcome, i.e. predict the outcome with sufficient accuracy. Successful applications of these models are expected to facilitate individualized medicine and improve healthcare outcome in general. There are various methods to develop the described models. Traditional statistical methods like for example logistic regression for binary data and Cox regression for survival data are widely used in medical literature. Another field which deals with similar classification problems is the machine learning field. This field has its origins in the artificial intelligence and is another approach to statistics; machine learning models try to find patterns in data and are designed to learn from examples. They can actually be trained to perform a specific task, like for example predicting time-to-survival for cancer patients. The teaching lecture will involve a step-by-step practical guide for the process of development of decision making models. The concepts involved are data collection, data preprocessing, data transformation, predictor selection, model training, performance evaluation and validation strategies. Successful examples of clinical decision making models will be presented and a tutorial on how to use and interpret them is provided. Clinical decision making using prediction models is currently gaining interest in the radiotherapy field. Disseminating knowledge and full transparency about clinical decision making is however still one of the keys to increase acceptance for this concept in the community and to implement it in clinical practice.},
  langid = {english},
  keywords = {*decision support system,*oncology,*radiotherapy,*society,artificial intelligence,cancer patient,classification,clinical decision making,clinical practice,clinical trial (topic),community,consensus,decision making,health care,human,information processing,logistic regression analysis,machine learning,medical literature,model,parameters,patient,personalized medicine,physician,prediction,proportional hazards model,statistical analysis,statistics,survival,teaching,treatment outcome,tumor}
}

@article{varriano_etal22,
  title = {Explainability of Radiomics through Formal Methods},
  author = {Varriano, G and Guerriero, P and Santone, A and Mercaldo, F and Brunese, L},
  year = {2022},
  month = jun,
  journal = {COMPUTER METHODS AND PROGRAMS IN BIOMEDICINE},
  volume = {220},
  issn = {0169-2607},
  doi = {10.1016/j.cmpb.2022.106824},
  abstract = {Background and Objective: Artificial Intelligence has proven to be effective in radiomics. The main problem in using Artificial Intelligence is that researchers and practitioners are not able to know how the predictions are generated. This is currently an open issue because results' explainability is advantageous in understanding the reasoning behind the model, both for patients than for implementing a feedback mechanism for medical specialists using decision support systems. Methods : Addressing transparency issues related to the Artificial Intelligence field, the innovative technique of Formal methods use a mathematical logic reasoning to produce an automatic, quick and reliable diagnosis. In this paper we analyze results given by the adoption of Formal methods for the diagnosis of the Coronavirus disease: specifically, we want to analyse and understand, in a more medical way, the meaning of some radiomic features to connect them with clinical or radiological evidences. Results : In particular, the usage of Formal methods allows the authors to do statistical analysis on the feature value distributions, to do pattern recognition on disease models, to generalize the model of a disease and to reach high performances of results and interpretation of them. A further step for explainability can be accounted by the localization and selection of the most important slices in a multi-slice approach. Conclusions : In conclusion, we confirmed the clinical significance of some First order features as Skewness and Kurtosis. On the other hand, we suggest to decline the use of the Minimum feature because of its intrinsic connection with the Computational Tomography exam of the lung.},
  langid = {english},
  keywords = {*Artificial Intelligence,*Radiology,Artificial intelligence,COVID-19,Explainability,FEATURES,Formal methods,Humans,IDIOPATHIC PULMONARY-FIBROSIS,Model checking,Radiomics,Tomography; X-Ray Computed}
}

@article{vashistha_etal21,
  title = {Barriers to {{Prescribing Targeted Therapies}} for {{Patients With NSCLC With Highly Actionable Gene Variants}} in the {{Veterans Affairs National Precision Oncology Program}}.},
  author = {Vashistha, Vishal and Armstrong, Jenna and Winski, David and Poonnen, Pradeep J and Hintze, Bradley and Price, Meghan and Snowdon, Jane L and Weeraratne, Dilhan and Brotman, David and Jackson, Gretchen P and Kelley, Michael J},
  year = {2021},
