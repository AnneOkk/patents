@article{yaghoobikarimu_azadi18,
  title = {Diagnosing the {{ADHD Using}} a {{Mixture}} of {{Expert Fuzzy Models}}},
  author = {Yaghoobi Karimu, Reza and Azadi, Sassan},
  year = {2018},
