@article{andersson_etal21,
  title = {Predicting Women with Depressive Symptoms Postpartum with Machine Learning Methods},
  author = {Andersson, Sam and Bathula, Deepti R. and Iliadis, Stavros I. and Walter, Martin and Skalkidou, Alkistis},
  year = {2021},
  month = apr,
  journal = {Scientific Reports},
  volume = {11},
  number = {1},
  pages = {7877},
  publisher = {{Nature Publishing Group}},
  issn = {2045-2322},
  doi = {10.1038/s41598-021-86368-y},
  abstract = {Postpartum depression (PPD) is a detrimental health condition that affects 12\% of new mothers. Despite negative effects on mothers’ and children’s health, many women do not receive adequate care. Preventive interventions are cost-efficient among high-risk women, but our ability to identify these is poor. We leveraged the power of clinical, demographic, and psychometric data to assess if machine learning methods can make accurate predictions of postpartum depression. Data were obtained from a population-based prospective cohort study in Uppsala, Sweden, collected between 2009 and 2018 (BASIC study, n\,=\,4313). Sub-analyses among women without previous depression were performed. The extremely randomized trees method provided robust performance with highest accuracy and well-balanced sensitivity and specificity (accuracy 73\%, sensitivity 72\%, specificity 75\%, positive predictive value 33\%, negative predictive value 94\%, area under the curve 81\%). Among women without earlier mental health issues, the accuracy was 64\%. The variables setting women at most risk for PPD were depression and anxiety during pregnancy, as well as variables related to resilience and personality. Future clinical models that could be implemented directly after delivery might consider including these variables in order to identify women at high risk for postpartum depression to facilitate individualized follow-up and cost-effectiveness.},
  copyright = {2021 The Author(s)},
  langid = {english},
  keywords = {Depression,Machine learning,Risk factors},
  file = {/Users/annekleine/Zotero/storage/XPVS8P4N/Andersson et al. - 2021 - Predicting women with depressive symptoms postpart.pdf;/Users/annekleine/Zotero/storage/5EFUJI4F/s41598-021-86368-y.html}
}

@article{antoniou_etal22,
  title = {Mental {{Health Diagnosis}}: {{A Case}} for {{Explainable Artificial Intelligence}}},
  shorttitle = {Mental {{Health Diagnosis}}},
  author = {Antoniou, Grigoris and Papadakis, Emmanuel and Baryannis, George},
  year = {2022},
  month = may,
  journal = {International Journal on Artificial Intelligence Tools},
  volume = {31},
  number = {03},
  pages = {2241003},
  publisher = {{World Scientific Publishing Co.}},
  issn = {0218-2130},
  doi = {10.1142/S0218213022410032},
  abstract = {Mental illnesses are becoming increasingly prevalent, in turn leading to an increased interest in exploring artificial intelligence (AI) solutions to facilitate and enhance healthcare processes ranging from diagnosis to monitoring and treatment. In contrast to application areas where black box systems may be acceptable, explainability in healthcare applications is essential, especially in the case of diagnosing complex and sensitive mental health issues. In this paper, we first summarize recent developments in AI research for mental health, followed by an overview of approaches to explainable AI and their potential benefits in healthcare settings. We then present a recent case study of applying explainable AI for ADHD diagnosis which is used as a basis to identify challenges in realizing explainable AI solutions for mental health diagnosis and potential future research directions to address these challenges.},
  keywords = {Explainable artificial intelligence,healthcare,mental health}
}

@article{arevian_etal20,
  title = {Clinical State Tracking in Serious Mental Illness through Computational Analysis of Speech},
  author = {Arevian, Armen C. and Bone, Daniel and Malandrakis, Nikolaos and Martinez, Victor R. and Wells, Kenneth B. and Miklowitz, David J. and Narayanan, Shrikanth},
  year = {2020},
  month = jan,
  journal = {PLOS ONE},
  volume = {15},
  number = {1},
  pages = {e0225695},
  publisher = {{Public Library of Science}},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0225695},
  abstract = {Individuals with serious mental illness experience changes in their clinical states over time that are difficult to assess and that result in increased disease burden and care utilization. It is not known if features derived from speech can serve as a transdiagnostic marker of these clinical states. This study evaluates the feasibility of collecting speech samples from people with serious mental illness and explores the potential utility for tracking changes in clinical state over time. Patients (n = 47) were recruited from a community-based mental health clinic with diagnoses of bipolar disorder, major depressive disorder, schizophrenia or schizoaffective disorder. Patients used an interactive voice response system for at least 4 months to provide speech samples. Clinic providers (n = 13) reviewed responses and provided global assessment ratings. We computed features of speech and used machine learning to create models of outcome measures trained using either population data or an individual’s own data over time. The system was feasible to use, recording 1101 phone calls and 117 hours of speech. Most (92\%) of the patients agreed that it was easy to use. The individually-trained models demonstrated the highest correlation with provider ratings (rho = 0.78, p{$<$}0.001). Population-level models demonstrated statistically significant correlations with provider global assessment ratings (rho = 0.44, p{$<$}0.001), future provider ratings (rho = 0.33, p{$<$}0.05), BASIS-24 summary score, depression sub score, and self-harm sub score (rho = 0.25,0.25, and 0.28 respectively; p{$<$}0.05), and the SF-12 mental health sub score (rho = 0.25, p{$<$}0.05), but not with other BASIS-24 or SF-12 sub scores. This study brings together longitudinal collection of objective behavioral markers along with a transdiagnostic, personalized approach for tracking of mental health clinical state in a community-based clinical setting.},
  langid = {english},
  keywords = {Acoustics,Bipolar disorder,Depression,Emotions,Language,Mental health and psychiatry,Semantics,Speech},
  file = {/Users/annekleine/Zotero/storage/MPNG4CF4/Arevian et al. - 2020 - Clinical state tracking in serious mental illness .pdf;/Users/annekleine/Zotero/storage/HIICRZLS/article.html}
}

