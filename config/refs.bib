  shorttitle = {Radiation {{Oncologists}}’ {{Perceptions}} of {{Adopting}} an {{Artificial Intelligence}}–{{Assisted Contouring Technology}}},
  author = {Zhai, Huiwen and Yang, Xin and Xue, Jiaolong and Lavender, Christopher and Ye, Tiantian and Li, Ji-Bin and Xu, Lanyang and Lin, Li and Cao, Weiwei and Sun, Ying},
