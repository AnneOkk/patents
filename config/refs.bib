@article{imel_etal19a,
  title = {Design Feasibility of an Automated, Machine-Learning Based Feedback System for Motivational Interviewing},
  author = {Imel, Zac E. and Pace, Brian T. and Soma, Christina S. and Tanana, Michael and Hirsch, Tad and Gibson, James and Georgiou, Panayiotis and Narayanan, Shrikanth and Atkins, David C.},
