@article{zhang_etal20,
  title = {Identification of Psychiatric-Disorder Subtypes from Functional-Connectivity Patterns in Resting-State Electroencephalography},
  author = {Zhang, Yu and Wu, Wei and Toll, Russell T. and Naparstek, Sharon and {Maron-Katz}, Adi and Watts, Mallissa and Gordon, Joseph and Jeong, Jisoo and Astolfi, Laura and Shpigel, Emmanuel and Longwell, Parker and Sarhadi, Kamron and {El-Said}, Dawlat and Li, Yuanqing and Cooper, Crystal and {Chin-Fatt}, Cherise and Arns, Martijn and Goodkind, Madeleine S. and Trivedi, Madhukar H. and Marmar, Charles R. and Etkin, Amit},
  year = {2020},
