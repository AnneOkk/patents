@article{jiao_etal12,
  title = {Single Nucleotide Polymorphisms Predict Symptom Severity of Autism Spectrum Disorder},
  author = {Jiao, Yun and Chen, Rong and Ke, Xiaoyan and Cheng, Lu and Chu, Kangkang and Lu, Zuhong and Herskovits, Edward H},
  year = {2012},
